<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.map.MapRestoActivity">

    <ImageView
        android:id="@+id/restoMap_background_imageView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        android:src="@drawable/resto_map_01"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/restoMap_level10_button"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="24dp"
                android:src="@drawable/button_jungle_level_unhover"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />

            <LinearLayout
                android:id="@+id/restoMap_level10_linearLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/restoMap_level10_button"
                app:layout_constraintStart_toStartOf="@+id/restoMap_level10_button"
                app:layout_constraintTop_toBottomOf="@+id/restoMap_level10_button"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/restoMap_level9_button"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="24dp"
                android:src="@drawable/button_jungle_level_unhover"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:id="@+id/restoMap_level9_linearLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/restoMap_level9_button"
                app:layout_constraintStart_toStartOf="@+id/restoMap_level9_button"
                app:layout_constraintTop_toBottomOf="@+id/restoMap_level9_button"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/restoMap_level8_button"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="24dp"
                android:src="@drawable/button_jungle_level_unhover"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:id="@+id/restoMap_level8_linearLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/restoMap_level8_button"
                app:layout_constraintStart_toStartOf="@+id/restoMap_level8_button"
                app:layout_constraintTop_toBottomOf="@+id/restoMap_level8_button"/>

            <ImageView
                android:id="@+id/restoMap_level6_button"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="60dp"
                android:src="@drawable/button_jungle_level_unhover"
                app:layout_constraintEnd_toStartOf="@+id/restoMap_level8_button"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:id="@+id/restoMap_level6_linearLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/restoMap_level6_button"
                app:layout_constraintStart_toStartOf="@+id/restoMap_level6_button"
                app:layout_constraintTop_toBottomOf="@+id/restoMap_level6_button"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/restoMap_level7_button"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="65dp"
                android:src="@drawable/button_jungle_level_unhover"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:id="@+id/restoMap_level7_linearLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/restoMap_level7_button"
                app:layout_constraintStart_toStartOf="@+id/restoMap_level7_button"
                app:layout_constraintTop_toBottomOf="@+id/restoMap_level7_button"/>

            <ImageView
                android:id="@+id/restoMap_level3_button"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginTop="16dp"
                android:src="@drawable/button_jungle_level_unhover"
                app:layout_constraintEnd_toStartOf="@+id/restoMap_level4_button"
                app:layout_constraintStart_toEndOf="@+id/restoMap_level2_button"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:id="@+id/restoMap_level3_linearLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/restoMap_level3_button"
                app:layout_constraintStart_toStartOf="@+id/restoMap_level3_button"
                app:layout_constraintTop_toBottomOf="@+id/restoMap_level3_button"/>

            <ImageView
                android:id="@+id/restoMap_level4_button"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginTop="16dp"
                android:src="@drawable/button_jungle_level_unhover"
                app:layout_constraintEnd_toStartOf="@+id/restoMap_level5_button"
                app:layout_constraintStart_toEndOf="@+id/restoMap_level3_button"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:id="@+id/restoMap_level4_linearLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/restoMap_level4_button"
                app:layout_constraintStart_toStartOf="@+id/restoMap_level4_button"
                app:layout_constraintTop_toBottomOf="@+id/restoMap_level4_button"/>

            <ImageView
                android:id="@+id/restoMap_level1_button"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginStart="24dp"
                android:layout_marginTop="16dp"
                android:src="@drawable/button_jungle_level_unhover"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:id="@+id/restoMap_level1_linearLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/restoMap_level1_button"
                app:layout_constraintStart_toStartOf="@+id/restoMap_level1_button"
                app:layout_constraintTop_toBottomOf="@+id/restoMap_level1_button"/>

            <ImageView
                android:id="@+id/restoMap_level2_button"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginTop="16dp"
                android:src="@drawable/button_jungle_level_unhover"
                app:layout_constraintEnd_toStartOf="@+id/restoMap_level3_button"
                app:layout_constraintStart_toEndOf="@+id/restoMap_level1_button"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:id="@+id/restoMap_level2_linearLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/restoMap_level2_button"
                app:layout_constraintStart_toStartOf="@+id/restoMap_level2_button"
                app:layout_constraintTop_toBottomOf="@+id/restoMap_level2_button"/>

            <ImageView
                android:id="@+id/restoMap_level5_button"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="45dp"
                android:src="@drawable/button_jungle_level_unhover"
                app:layout_constraintEnd_toStartOf="@+id/restoMap_level7_button"
                app:layout_constraintStart_toEndOf="@+id/restoMap_level4_button"
                app:layout_constraintTop_toTopOf="parent" />

            <LinearLayout
                android:id="@+id/restoMap_level5_linearLayout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/restoMap_level5_button"
                app:layout_constraintStart_toStartOf="@+id/restoMap_level5_button"
                app:layout_constraintTop_toBottomOf="@+id/restoMap_level5_button"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>

    <ImageView
        android:id="@+id/restoMap_backButton_imageView"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back_button"
        app:tint="#FA9238" />
</androidx.constraintlayout.widget.ConstraintLayout>